{
  "name": "typescript-eslint-plugin",
  "version": "1.0.8",
  "description": "ESLint language-service plugin for TypeScript",
  "author": "Manuel Thalmann <m@nuth.ch>",
  "license": "MIT",
  "keywords": [
    "TypeScript",
    "ESLint",
    "language-service",
    "lint",
    "plugin",
    "TypeScript-plugin"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "rebuild": "npm run clean && npm run build",
    "watch": "tsc -p . --watch",
    "clean": "rimraf ./lib",
    "lint": "eslint --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./src",
    "lint-compact": "npm run lint -- -f compact || exit 0",
    "test": "mocha",
    "prepare": "npm run rebuild"
  },
  "dependencies": {
    "@thi.ng/cache": "^1.0.34",
    "lodash.clonedeep": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "mock-require": "^3.0.3",
    "upath": "^1.2.0",
    "vscode-languageserver": "^6.1.1"
  },
  "devDependencies": {
    "@manuth/tsconfig": "^1.2.2",
    "@manuth/tslint-presets": "^1.0.4",
    "@types/eslint": "^6.1.8",
    "@types/find-up": "^4.0.0",
    "@types/fs-extra": "^8.1.0",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.pick": "^4.4.6",
    "@types/mocha": "^7.0.2",
    "@types/mock-require": "^2.0.0",
    "@types/node": "^13.9.1",
    "@types/npm-which": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.0.1",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "fs-extra": "^8.1.0",
    "lodash.isequal": "^4.5.0",
    "mocha": "^7.1.0",
    "npm-which": "^3.0.1",
    "random-js": "^2.1.0",
    "rimraf": "^3.0.2",
    "temp-filesystem": "^1.1.3",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "typescript-tslint-plugin": "^0.5.5"
  }
}
